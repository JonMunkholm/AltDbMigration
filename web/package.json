{
  "name": "schema-visualizer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "esbuild src/app.ts --bundle --outfile=dist/bundle.js --format=iife --target=es2020 --minify",
    "build:dev": "esbuild src/app.ts --bundle --outfile=dist/bundle.js --format=iife --target=es2020 --sourcemap",
    "watch": "esbuild src/app.ts --bundle --outfile=dist/bundle.js --format=iife --target=es2020 --sourcemap --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/cytoscape": "^3.21.0",
    "esbuild": "^0.20.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "cytoscape": "^3.28.0",
    "cytoscape-cose-bilkent": "^4.1.0",
    "cytoscape-dagre": "^2.5.0",
    "dagre": "^0.8.5"
  }
}
